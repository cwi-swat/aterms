module Character-Class-Membership
imports
        Character-Class-Syntax
        Character-Arithmetic
exports
  context-free syntax
    Character "in" OptCharRanges -> Bool
    Character "in" CharClass     -> Bool
    CharClass "sub" CharClass    -> Bool
    CharClass "equiv" CharClass  -> Bool

hiddens
  variables
    "_c"[0-9\']*    -> Character
    "cr"[0-9\']*    -> CharRange
    "cr""*"[0-9\']* -> OptCharRanges
    "cr""+"[0-9\']* -> CharRanges
    "cc"[0-9\']*    -> CharClass
