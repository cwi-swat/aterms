module Sdf2-to-C-API

imports Sdf2-to-ADT Gen-C-API

exports
  context-free syntax
    "sdf2-to-c-api"  ":" Module 		-> C-API

hiddens
  context-free syntax
    modname-to-filename(ModuleName)		-> C-FILE-NAME

  variables
    "$ADT"		-> ATerm
    "$CAPI"		-> C-API
    "$Char*"		-> CHAR*
    "$FileName"		-> C-FILE-NAME
    "$ImpSection*"	-> ImpSection*
    "$Module"		-> Module
    "$ModName"		-> ModuleName
    "$Section*"		-> Section*
